                    "                                                    var items = {",
                    "                                                        orderId : responseData.items[i].orderId,",
                    "                                                        productId : responseData.items[i].productId,",
                    "                                                        productName : responseData.items[i].productName,",
                    "                                                        productDescription : responseData.items[i].productDescription,",
